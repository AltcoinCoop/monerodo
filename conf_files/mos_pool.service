[Unit]
Description=Monero Pool Server
After=network.target mos_wallet.service mos_changeip.service network-online.target
Requires=mos_wallet.service
Wants=network-online.target

[Service]
User=monrig
Group=monrig
WorkingDirectory=~

Type=simple
ExecStart=/home/monrig/.monerodo/run_pool.sh                           

[Install]
WantedBy=multi-user.target

