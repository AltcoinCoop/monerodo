[Unit]
Description=Monero Solo Pool Miner
After=network.target mos_pool.service mos_changeip.service network-online.target
Requires=mos_pool.service
Wants=network-online.target

[Service]
User=monrig
Group=monrig
WorkingDirectory=~

Type=simple
ExecStart=/home/monrig/.monerodo/run_solo_miner.sh

[Install]
WantedBy=multi-user.target

